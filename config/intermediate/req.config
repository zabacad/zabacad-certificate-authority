# Only differences from the config/root/req.config are commented.
#
# vi:syntax=dosini

[ req ]
utf8   = yes
prompt = no

default_md = sha512

distinguished_name = req_dn
attributes         = req_attrib

[ req_dn ]
# The root and intermediate certificate are often named similarly.
commonName = Example intermediate CA G2

[ req_attrib ]
